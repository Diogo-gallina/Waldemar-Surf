{
  "name": "waldemar-surf",
  "version": "1.0.0",
  "description": "Typescript Node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "test": "yarn test:unit && yarn test:functional",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Diogo-gallina/Waldemar-Surf.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Diogo Gallina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Diogo-gallina/Waldemar-Surf/issues"
  },
  "homepage": "https://github.com/Diogo-gallina/Waldemar-Surf#readme",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "axios": "^1.6.4",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "config": "^3.3.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongo": "^0.1.0",
    "mongoose": "^8.0.3",
    "pino": "^8.17.2",
    "rm": "^0.1.8",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/config": "^3.3.3",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/module-alias": "^2.0.4",
    "@types/mongoose": "^5.11.97",
    "@types/nock": "^11.1.0",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nock": "^13.4.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0"
  }
}
